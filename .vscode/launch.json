{
	"version": "0.2.0",
	"configurations": [

    {
      "type": "node",
      "request": "launch",
      "name": "Node NeatBin",
      "program": "${workspaceFolder}/src/index.js",
      "sourceMaps": true,
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "outFiles": ["${workspaceFolder}/.vscode/out/*"],
      "cwd": "${workspaceFolder}",
      "stopOnEntry": false,
      "runtimeExecutable": null,
      "env": {
          "NODE_ENV": "testing"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Node X NeatBin",
      "program": "${workspaceFolder}/src/nxt/index.js",
      "sourceMaps": true,
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "outFiles": ["${workspaceFolder}/.vscode/out/*"],
      "cwd": "${workspaceFolder}",
      "stopOnEntry": false,
      "runtimeExecutable": null,
      "env": {
          "NODE_ENV": "testing"
      }
    },
	]
}